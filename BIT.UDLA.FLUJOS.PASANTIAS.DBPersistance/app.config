            <setting name="SeleccionarAlumno" serializeAs="String">
                <value>SELECT      alum_rut, alum_dv, alum_nmat, alum_carr, alum_plan, alum_espe, alum_jorn,
                  pers_nomb, pers_apat, pers_amat, pers_sexo, pers_eciv, pers_fnac,
